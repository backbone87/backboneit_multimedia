{
	"name": "hofff/contao-multimedia",
	"description": "Embed multimedia content with various players into Contao CMS",
	"keywords": [
		"video",
		"player",
		"multimedia",
		"contao"
	],
	"type": "contao-module",
	"license": "LGPL-3.0+",
	"homepage": "https://hofff.com/",
	"authors": [
		{
			"name": "Oliver Hoff",
			"email": "oliver@hofff.com",
			"homepage": "http://hofff.com/",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/hofff/contao-multimedia/issues",
		"source": "https://github.com/hofff/contao-multimedia"
